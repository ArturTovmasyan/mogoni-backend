{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "1.6.0",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/orm": "^2.5.11",
        "nelmio/cors-bundle": "1.5.4",
        "sensio/framework-extra-bundle": "5.2.2",
        "jms/serializer-bundle": "2.4.2",
        "sensiolabs/security-checker": "^5.0",
        "sonata-project/admin-bundle": "3.42.2",
        "sonata-project/core-bundle": "3.10.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "stof/doctrine-extensions-bundle": "1.3.0",
        "symfony/apache-pack": "1.0.1",
        "symfony/console": "4.2.7",
        "symfony/dotenv": "4.2.7",
        "symfony/flex": "1.1.8",
        "symfony/framework-bundle": "4.2.7",
        "symfony/maker-bundle": "1.9.0",
        "symfony/routing": "4.2.7",
        "symfony/security-core": "4.2.7",
        "symfony/validator": "4.2.7",
        "symfony/yaml": "4.2.7",
        "symfony/twig-bundle": "4.2.7"
    },
    "require-dev": {
        "symfony/profiler-pack": "1.0.4",
        "symfony/var-dumper": "4.2.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "compile": [
            "rm -rf var/cache/*",
            "php bin/console d:s:u --force"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.7"
        }
    }
}
